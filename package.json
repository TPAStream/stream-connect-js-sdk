    "jest": "^26.6.3",
    "marked": "^4.1.0",
    "prettier": "^1.18.2",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "transform-loader": "^0.2.3",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^4.2.2"
