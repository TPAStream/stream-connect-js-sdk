  "dependencies": {
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.6",
    "axios": "^0.21.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "query-string": "^6.6.0",
    "react": "^16.11.0",
